{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "vadd_vadd_sw_emu",
      "type": "gdb",
      "request": "launch",
      "target": "${workspaceFolder}/vadd/build_sw_emu/vadd/vadd",
      "arguments": "${workspaceFolder}/vadd/build_sw_emu/hw_link/binary_container_1.xclbin",
      "cwd": "${workspaceFolder}/vadd/build_sw_emu/vadd_vadd_sw_emu",
      "gdbpath": "${env:XILINX_VITIS}/bin/xgdb",
      "valuesFormatting": "prettyPrinters",
      "env": {
        "XILINX_VITIS": "${env:XILINX_VITIS}",
        "XILINX_HLS": "${env:XILINX_VITIS}",
        "XCL_EMULATION_MODE": "sw_emu",
        "ENABLE_KERNEL_DEBUG": "true",
        "XRT_INI_PATH": "${workspaceFolder}/vadd/_ide/sw_emu/xrt.ini"
      }
    },
    {
      "name": "vadd_vadd_hw_emu",
      "type": "gdb",
      "request": "launch",
      "target": "${workspaceFolder}/vadd/build_hw_emu/vadd/vadd",
      "arguments": "${workspaceFolder}/vadd/build_hw_emu/hw_link/binary_container_1.xclbin",
      "cwd": "${workspaceFolder}/vadd/build_hw_emu/vadd_vadd_hw_emu",
      "gdbpath": "${env:XILINX_VITIS}/bin/xgdb",
      "valuesFormatting": "prettyPrinters",
      "env": {
        "XILINX_VITIS": "${env:XILINX_VITIS}",
        "XILINX_HLS": "${env:XILINX_VITIS}",
        "XCL_EMULATION_MODE": "hw_emu",
        "HW_EM_LAUNCH_WAVEFORM": "gui",
        "XRT_INI_PATH": "${workspaceFolder}/vadd/_ide/hw_emu/xrt.ini"
      }
    },
    {
      "name": "vadd_vadd_hw",
      "type": "gdb",
      "request": "launch",
      "target": "${workspaceFolder}/vadd/build_hw/vadd/vadd",
      "arguments": "${workspaceFolder}/vadd/build_hw/hw_link/binary_container_1.xclbin",
      "cwd": "${workspaceFolder}/vadd/build_hw/vadd_vadd_hw",
      "gdbpath": "${env:XILINX_VITIS}/bin/xgdb",
      "valuesFormatting": "prettyPrinters",
      "env": {
        "XILINX_VITIS": "${env:XILINX_VITIS}",
        "XILINX_HLS": "${env:XILINX_VITIS}",
        "XRT_INI_PATH": "${workspaceFolder}/vadd/_ide/hw/xrt.ini"
      }
    }
  ]
}